{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/tenants/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/managers/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/properties/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/applications/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/leases/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
} 